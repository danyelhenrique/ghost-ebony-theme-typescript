{
  "name": "frontier",
  "description": "A minimal starter theme for the Ghost blogging platform, featuring Webpack, Browsersync, and Bootstrap.",
  "demo": "https://frontier.jamesfacts.com/",
  "repository": "https://github.com/jamesfacts/frontier-ghost-theme",
  "version": "4.7.2",
  "license": "MIT",
  "engines": {
    "ghost": ">=4.0.0",
    "ghost-api": "v4"
  },
  "author": {
    "name": "James White",
    "email": "james@jfwhite.org",
    "url": "https://jamesfacts.com"
  },
  "scripts": {
    "build": "webpack  --progress --config ./webpack/webpack.config.js",
    "build:prod": "webpack  --progress -p --config ./webpack/webpack.config.js",
    "start": "webpack  --watch --config ./webpack/webpack.config.js --mode development",
    "ghost:restart": "set NODE_ENV=development &&  ghost run"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/runtime": "^7.16.3",
    "@types/bootstrap": "^5.1.8",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.13",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "precss": "^4.0.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "config": {
    "posts_per_page": 14,
    "image_sizes": {},
    "card_assets": true
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Responsive",
      "Starter theme",
      "Boilerplate"
    ]
  },
  "keywords": [
    "responsive",
    "ghost",
    "starter",
    "theme",
    "boilerplate",
    "bootstrap",
    "ghost-theme"
  ],
  "dependencies": {
    "@glidejs/glide": "^3.5.2",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "popper.js": "^1.12.9"
  }
}
